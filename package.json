{
  "name": "vue-monaco-ts",
  "version": "1.0.1",
  "description": "MonacoEditor component for Vue.js",
  "repository": {
    "url": "https://github.com/meta-exception/vue-monaco-ts/",
    "type": "git"
  },
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.ts --open",
    "build": "vue-cli-service build --name vue-monaco-ts --entry ./src/index.ts --target lib",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "vue-cli-service test:unit",
    "cleanup": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "monaco-editor": "^0.14.3",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.0.4",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuedoc/md": "^1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "jest": "^23.6.0",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "vue-cli-plugin-component": "^1.7.1",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "keywords": [
    "vue",
    "component",
    "monaco",
    "monaco-editor"
  ],
  "author": "James Bruno",
  "license": "MIT",
  "main": "dist/vue-monaco-ts.umd.js",
  "module": "dist/vue-monaco-ts.common.min.js"
}
